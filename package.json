{
    "name": "tracker-cljs",
    "version": "1.0.0",
    "description": "TimeTracker",
    "main": "resources/main.js",
    "scripts": {
        "dev": "sass --watch resources/public/sass:resources/public/css & ./node_modules/.bin/shadow-cljs watch main renderer",
        "test": "./node_modules/.bin/shadow-cljs compile main renderer test",
        "build": "./node_modules/.bin/shadow-cljs compile main release",
        "clean": "rm -rf resources/public/js/* && rm -rf target",
        "dist": "electron-builder"
    },
    "keywords": [
        "shadow-cljs",
        "electron"
    ],
    "author": "Rodion Shylo <r.shylo@softarex.com>",
    "license": "MIT",
    "devDependencies": {
        "electron": "10.1.1",
        "electron-builder": "21.2.0",
        "shadow-cljs": "2.8.110"
    },
    "dependencies": {
        "@date-io/moment": "1.3.13",
        "@material-ui/core": "4.11.0",
        "@material-ui/icons": "4.9.1",
        "@material-ui/lab": "4.0.0-alpha.56",
        "@material-ui/pickers": "3.2.10",
        "@material-ui/styles": "4.10.0",
        "@shopify/draggable": "^1.0.0-beta.8",
        "axios": "0.20.0",
        "create-react-class": "15.6.3",
        "electron-log": "^4.3.0",
        "express": "4.17.1",
        "moment": "2.27.0",
        "react": "16.13.1",
        "react-beautiful-dnd": "13.0.0",
        "react-dnd": "11.1.3",
        "react-dom": "16.13.1",
        "react-draggable": "^4.4.3",
        "react-simple-timefield": "3.2.3",
        "sass": "1.26.10",
        "ws": "^7.4.0"
    },
    "build": {
        "productName": "TaskTracker",
        "appId": "TaskTracker",
        "win": {
            "target": "NSIS",
            "icon": "build/icon.png"
        },
        "dmg": {
            "icon": "build/icon.icns",
            "contents": [
                {
                    "x": 110,
                    "y": 150
                },
                {
                    "x": 400,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "linux": {
            "icon": "build/icon.icns",
            "target": [
                "AppImage",
                "deb"
            ]
        }
    }
}
